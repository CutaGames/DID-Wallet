            <RelativeLayout
                android:id="@+id/upload_picture_select_folder_body_item_layout_1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/upload_picture_select_folder_body_item_height"
                android:layout_marginTop="@dimen/upload_picture_select_folder_body_item_margin_top"
                android:background="@drawable/shadow_bg_bottom_line">
                <ImageView
                    android:id="@+id/upload_picture_select_folder_body_item_icon_1"
                    android:layout_width="@dimen/upload_picture_select_folder_body_item_icon_width"
                    android:layout_height="@dimen/upload_picture_select_folder_body_item_icon_height"
                    android:src="@mipmap/upload_picture_select_folder_body_icon_default_1"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/upload_picture_select_folder_body_icon_margin_left"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/picture_time_year_default_text_1"
                    android:textSize="@dimen/upload_picture_select_folder_body_item_title_text_size"
                    android:textColor="@color/upload_picture_select_folder_body_item_title_text_color"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="@dimen/upload_picture_select_folder_body_item_title_margin_top"
                    android:layout_toRightOf="@id/upload_picture_select_folder_body_item_icon_1"
                    android:layout_marginLeft="@dimen/upload_picture_select_folder_body_item_title_margin_left"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/upload_picture_select_folder_body_item_number_text_default_1"
                    android:textSize="@dimen/upload_picture_select_folder_body_item_number_text_size"
                    android:textColor="@color/upload_picture_select_folder_body_item_number_text_color"
                    android:layout_alignParentBottom="true"
                    android:layout_marginBottom="@dimen/upload_picture_select_folder_body_item_number_text_margin_bottom"
                    android:layout_toRightOf="@id/upload_picture_select_folder_body_item_icon_1"
                    android:layout_marginLeft="@dimen/upload_picture_select_folder_body_item_title_margin_left"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/main_mine_item_arrow_icon"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/upload_picture_select_folder_body_item_arrow_margin_right"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/upload_picture_select_folder_body_item_layout_2"
