<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!-- year layout start -->
    <LinearLayout
        android:id="@+id/picture_time_year_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="visible">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/upload_picture_select_folder_body_margin_left"
            android:layout_marginRight="@dimen/upload_picture_select_folder_body_margin_right"
            android:layout_marginTop="@dimen/upload_picture_select_folder_body_margin_top"
